  "require": {
    "wpackagist-plugin/timber-library": "1.18.2",
    "perimetre/advanced-custom-fields-pro": "*",
    "perimetre/gravityforms": "*",
    "perimetre/gravityformsmailchimp": "*",
    "perimetre/gravityformsrecaptcha": "*",
    "perimetre/gravityformssendgrid": "*",
    "perimetre/sitepress-multilingual-cms": "*",
    "perimetre/wpml-media-translation": "*",
    "perimetre/wpml-string-translation": "*",
    "perimetre/wpml-translation-management": "*"
  },
  "autoload": {
    "psr-0": {
      "Perimetre": "src/"
    }
  },
  "extra": {
    "installer-paths": {
      "wp-content/mu-plugins/{$name}/": [],
      "wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  }
